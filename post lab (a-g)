import numpy as np

a = np.arange(2, 11).reshape(3, 3)
print(a)

b = np.array([1, 2, 3, 4, 5])
print(b[::-1])

c1 = np.array([1, 2, 3, 4, 5])
c2 = np.array([4, 5, 6, 7, 8])
print(np.intersect1d(c1, c2))

d = np.array([1, 2, 3])
print(np.repeat(d, 2))

e = np.array([1, 2, 3, 4, 5])
print(e.size * e.itemsize)

f1 = np.ones((3, 3))
f2 = np.zeros((3, 3))
print(f1)
print(f2)

g = np.array([10, 20, 30, 40, 50])
print(g[3])
